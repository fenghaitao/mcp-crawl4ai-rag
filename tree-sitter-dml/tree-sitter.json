{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "dml",
      "camelcase": "Dml",
      "title": "DML",
      "scope": "source.dml",
      "file-types": [
        "dml"
      ],
      "injection-regex": "^dml$",
      "class-name": "TreeSitterDml"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "DML (Device Modeling Language) 1.4 grammar for tree-sitter - used in Intel Simics for hardware device modeling",
    "authors": [
      {
        "name": "wangli-ustc"
      }
    ],
    "links": {
      "repository": "https://github.com/wangli-ustc/tree-sitter-dml"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": false
  }
}
