{
  "name": "tree-sitter-dml",
  "version": "0.1.0",
  "description": "DML (Device Modeling Language) grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node/index.d.ts",
  "keywords": [
    "parser",
    "tree-sitter",
    "dml",
    "device-modeling",
    "simics"
  ],
  "author": "wangli-ustc",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.22.6",
    "prebuildify": "^6.0.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "tree-sitter": [
    {
      "scope": "source.dml",
      "file-types": [
        "dml"
      ]
    }
  ]
}
